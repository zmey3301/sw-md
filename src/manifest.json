{
  "manifest_version": 2,
  "name": "SmartsWorld MD",
  "description": "Markdown компилятор для форума SmartsWorld",
  "icons": {
    "32": "icons/32.png",
    "128": "icons/128.png"
  },
  "content_scripts": [{
    "matches": ["*://forum.smartsworld.ru/view/*", "*://forum.smartsworld.ru/conversations/*", "*://forum.smartsworld.ru/pages/*"]
  }],
  "permissions": [
    "activeTab",
    "declarativeContent"
  ]
}
